{
  "name": "otr",
  "version": "1.0.0",
  "description": "URL Shortner",
  "main": "server.js",
  "dependencies": {
    "bcryptjs": "*",
    "body-parser": "*",
    "connect-redis": "*",
    "cookie-parser": "*",
    "express": "*",
    "express-session": "*",
    "morgan": "*",
    "normalize-url": "*",
    "nunjucks": "*",
    "ioredis": "*",
    "rndm": "*",
    "serve-favicon": "*",
    "validator": "*",
    "csurf": "*"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --silent=false --verbose=true --runInBand",
    "start": "node server.js"
  },
  "author": "Arpit Jain",
  "license": "ISC",
  "devDependencies": {
    "jest": "^24.8.0",
    "supertest": "^4.0.2"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "lcov"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test/_not_used/",
      "<rootDir>/node_modules/"
    ]
  }
}
